<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="gene_impact_medicine_tree" model="ir.ui.view">
            <field name="name">gene impact tree</field>
            <field name="model">gene.impact.medicine</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="medicine_code"/>
                </tree>
            </field>
        </record>

        <record id="gene_impact_medicine_form" model="ir.ui.view">
            <field name="name">gene impact form</field>
            <field name="model">gene.impact.medicine</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="medicine_code"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="gene_impact_medicine_action" model="ir.actions.act_window">
            <field name="name">gene_impact_medicine</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">gene.impact.medicine</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                <!-- Add Text Here -->
              </p><p>
                <!-- More details about what a user can do with this object will be OK -->
              </p>
            </field>
        </record>

    </data>
</odoo>